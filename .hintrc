{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/is-valid": "off",
    "button-type": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scroll-behavior"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ]
  }
}